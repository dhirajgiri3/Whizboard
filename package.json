{
  "name": "whizboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "dev:fast": "NODE_OPTIONS='--inspect' next dev | pino-pretty",
    "dev:turbo": "NEXT_TURBO=1 next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "node -r dotenv/config node_modules/vitest/vitest.mjs",
    "test:ui": "node -r dotenv/config node_modules/vitest/vitest.mjs --ui",
    "test:run": "node -r dotenv/config node_modules/vitest/vitest.mjs run",
    "test:sim": "node -r dotenv/config scripts/multi-user-simulation.js",
    "test:visual": "node -r dotenv/config node_modules/vitest/vitest.mjs run **/*.visual.test.{ts,tsx}",
    "test:visual:update": "node -r dotenv/config node_modules/vitest/vitest.mjs run **/*.visual.test.{ts,tsx} -- -u",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:report": "npx playwright show-report",
    "security:audit": "npm audit && npm audit fix",
    "security:check": "snyk test",
    "security:monitor": "snyk monitor",
    "migrate:users": "node -r dotenv/config scripts/migrate-users.js",
    "migrate:users:dry-run": "node -r dotenv/config scripts/migrate-users.js --dry-run",
    "migrate:users:force": "node -r dotenv/config scripts/migrate-users.js --force"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/experimental-nextjs-app-support": "^0.12.2",
    "@next-auth/mongodb-adapter": "^1.1.3",
