    "@pothos/plugin-dataloader": "^4.4.2",
    "@pothos/plugin-relay": "^4.5.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@sendgrid/mail": "^8.1.5",
    "@studio-freight/lenis": "^1.0.42",
    "@types/ws": "^8.18.1",
    "@upstash/redis": "^1.35.0",
    "canvas": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dns": "^0.2.2",
    "dotenv": "^17.2.1",
    "events": "^3.3.0",
    "express-session": "^1.18.1",
    "framer-motion": "^12.23.9",
    "fs": "^0.0.1-security",
    "googleapis": "^144.0.0",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.5",
    "graphql-yoga": "^5.13.5",
    "gsap": "^3.13.0",
    "helmet": "^8.0.0",
    "jspdf": "^3.0.1",
    "konva": "^9.3.20",
    "lenis": "^1.3.8",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.525.0",
    "mongodb": "^5.9.2",
    "nanoid": "^5.1.5",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pino": "^9.7.0",
